package gpa;

public class CalculateGPA {

	public static void main(String[] args) {
		CalculateGPA stGPA = new CalculateGPA();
		String gpa = stGPA.getGPA(80);
		
		System.out.println(gpa);

	}

	public String getGPA(int marks) {
		if (marks >= 80) {
			return "A+";
		} else if (marks >= 75) {
			return "A";
		} else if (marks >= 70) {
			return "A-";
		} else if (marks >= 65) {
			return "B+";
		} else if (marks >= 60) {
			return "B";
		} else if (marks >= 55) {
			return "B-";
		} else if (marks >= 50) {
			return "C+";
		} else if (marks >= 45) {
			return "C";
		} else if (marks >= 40) {
			return "D";
		} else {
			return "F";
		}
	}
}
